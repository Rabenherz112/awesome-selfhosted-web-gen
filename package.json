  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Rabenherz112/awesome-selfhosted-web-gen/issues"
  },
  "homepage": "https://github.com/Rabenherz112/awesome-selfhosted-web-gen#readme",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "tailwindcss": "^4.1.11"
  }
}
